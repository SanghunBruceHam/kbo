{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": ["error", {
      "vars": "all",
      "args": "after-used",
      "ignoreRestSiblings": false,
      "varsIgnorePattern": "^_",
      "argsIgnorePattern": "^_"
    }],
    "no-unused-expressions": "error",
    "no-unreachable": "error",
    "no-dead-code": "off"
  },
  "globals": {
    "Chart": "readonly",
    "gtag": "readonly",
    "PartnersCoupang": "readonly"
  }
}
